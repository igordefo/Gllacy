{
  "name": "cat-energy",
  "version": "13.0.0",
  "private": true,
  "description": "Личный проект «Кэт энерджи» от HTML Academy",
  "devDependencies": {
    "@htmlacademy/editorconfig-cli": "1.x",
    "autoprefixer": "8.6.x",
    "browser-sync": "2.27.x",
    "del": "^3.0.0",
    "gh-pages": "^1.2.0",
    "gulp": "3.9.x",
    "gulp-csso": "^3.0.1",
    "gulp-htmlmin": "^4.0.0",
    "gulp-imagemin": "^4.1.0",
    "gulp-jsmin": "^0.1.5",
    "gulp-plumber": "1.2.x",
    "gulp-postcss": "7.0.x",
    "gulp-rename": "^1.3.0",
    "gulp-sass": "4.0.x",
    "gulp-svgstore": "^6.1.1",
    "gulp-webp": "^3.0.0",
    "run-sequence": "^2.2.1",
    "stylelint": "9.3.x",
    "stylelint-config-htmlacademy": "0.1.x"
  },
  "scripts": {
    "editorconfig": "editorconfig-cli",
    "stylelint": "stylelint \"source/sass/**/*.scss\" --syntax scss",
    "test": "npm run editorconfig && npm run stylelint",
    "build": "gulp build",
    "start": "npm run build && gulp serve",
    "deploy": "gh-pages -d build"
  },
  "browserslist": [
    "last 2 versions",
    "IE 11",
    "Firefox ESR"
  ],
  "editorconfig-cli": [
    "*.json",
    "*.js",
    "source/*.html",
    "source/js/**/*.js",
    "source/img/**/*.svg",
    "source/sass/**/*.{sass,scss}"
  ],
  "engines": {
    "node": "8.9"
  }
}
